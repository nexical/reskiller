{
  "name": "@nexical/reskill",
  "version": "0.0.0",
  "description": "Adaptive Skill Learning System for the Nexus Ecosystem",
  "main": "dist/index.js",
  "bin": {
    "reskill": "dist/index.js",
    "prompt": "dist/cli/prompt.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*": [
      "prettier --write --ignore-unknown"
    ],
    "**/*.{js,jsx,ts,tsx,astro}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "glob": "^10.3.10",
    "inquirer": "^9.2.14",
    "minimist": "^1.2.8",
    "nunjucks": "^3.2.4",
    "ora": "^8.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "typescript-eslint": "^8.54.0"
  },
  "types": "dist/index.d.ts"
}
